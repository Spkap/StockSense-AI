# StockSense Agent - Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# Backend API Keys (Required)
# =============================================================================

# Google Gemini API Key - Get from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY="your_google_api_key_here"

# NewsAPI Key - Get from https://newsapi.org/register
NEWSAPI_KEY="your_newsapi_key_here"

# =============================================================================
# Supabase (Required for User Features)
# =============================================================================

# Supabase Project URL
SUPABASE_URL="https://your-project.supabase.co"

# Supabase Anonymous Key (public, safe for frontend)
SUPABASE_ANON_KEY="your_supabase_anon_key_here"

# Supabase Service Key (private, backend only - DO NOT expose)
SUPABASE_SERVICE_KEY="your_supabase_service_key_here"

# =============================================================================
# Frontend Environment (for Vite - prefix with VITE_)
# =============================================================================

# Copy these to frontend/.env as well
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your_supabase_anon_key_here"
VITE_API_URL="http://127.0.0.1:8000"
